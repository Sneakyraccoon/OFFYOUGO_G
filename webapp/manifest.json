{
    "sap.ui5": {
      "rootView": {
        "viewName": "ui5.vacation.view.App",
        "type": "XML"
      },
      "routing": {
        "config": {
          "routerClass": "sap.m.routing.Router",
          "viewType": "XML",
          "viewPath": "ui5.vacation.view",
          "controlId": "app",
          "controlAggregation": "pages",
          "bypassed": {
            "target": "login"
          }
        },
        "routes": [
          {
            "pattern": "",
            "name": "login",
            "target": "login"
          },
          {
            "pattern": "vacation",
            "name": "vacationRequest",
            "target": "vacationRequest"
          },
          {
            "pattern": "manager",
            "name": "managerInbox",
            "target": "managerInbox"
          }
        ],
        "targets": {
          "login": {
            "viewName": "Login"
          },
          "vacationRequest": {
            "viewName": "VacationRequest"
          },
          "managerInbox": {
            "viewName": "ManagerInbox"
          }
        }
      }
    }
  }
  